{
	"compilerOptions": {
		"baseUrl": "",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2017",
			"dom"
		],
		"mapRoot": "./",
		"module": "es6",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"outDir": "../dist/out-tsc",
		"sourceMap": true,
		"strictNullChecks": true,
		"target": "es5",
		"typeRoots": [
			"../node_modules/@types"
		]
	}
}
