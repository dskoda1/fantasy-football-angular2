{
  "name": "angular-lab",
  "version": "0.0.0",
  "description": "Angular Laboratory",
  "keywords": [
    "angular 2",
    "angular 2 seed",
    "angular 2 demo app",
    "angular 2 example",
    "angular"
  ],
  "homepage": "https://angular-lab.nomadoid.io",
  "license": "MIT",
  "author": {
    "name": "Roland Groza",
    "email": "rolandjitsu@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rolandjitsu/angular-lab.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "ng serve --open --environment=prod",
    "build": "ng build --sourcemaps --output-hashing=bundles",
    "build:prod": "ng build --target=production --environment=prod --output-hashing=bundles --no-progress",
    "lint": "ng lint --format stylish",
    "test": "ng test --single-run",
    "test:watch": "ng test -watch",
    "test:ci": "ng test --no-progress --browsers SL_CHROME,SL_SAFARI10,SL_FIREFOX,SL_IE11 --single-run",
    "e2e": "ng e2e --no-watch",
    "e2e:ci": "ng e2e --no-progress --no-watch --target=production --environment=prod",
    "deploy": "npm run build:prod && firebase deploy --token ${FIREBASE_TOKEN}",
    "deploy:ci": "npm run build:prod && scripts/ci/deploy.sh"
  },
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/flex-layout": "latest",
    "@angular/forms": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/material": "latest",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "@turf/helpers": "^3.7.5",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.2.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "latest",
    "@angular/compiler-cli": "^2.4.8",
    "@angular/tsc-wrapped": "latest",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.43",
    "@types/jasmine-expect": "^2.0.29",
    "@types/node": "^6.0.48",
    "codelyzer": "^2.0.0-beta.4",
    "firebase-tools": "^3.3.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jasmine-expect": "^3.0.1",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sauce-launcher": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.13",
    "protractor": "^5.1.1",
    "protractor-accessibility-plugin": "^0.3.0",
    "protractor-console-plugin": "^0.1.1",
    "ts-helpers": "^1.1.2",
    "ts-node": "^1.7.0",
    "tslint": "^4.4.2",
    "typescript": "2.0.10",
    "yargs": "^6.6.0",
    "concurrently": "^3.2.0"
  },
  "engines": {
    "node": ">= 7.4"
  }
}
